<?xml version="1.0"?>
<interface>
  <!-- interface-requires gtk+ 2.12 -->
  <!-- interface-naming-policy project-wide -->
  <object class="GtkWindow" id="toplevel">
    <property name="title" translatable="yes">Initializing Mayhem Launcher...</property>
    <property name="resizable">False</property>
    <property name="window_position">center</property>
    <child>
      <object class="GtkVBox" id="vbox1">
        <property name="visible">True</property>
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkImage" id="img_splash">
            <property name="width_request">500</property>
            <property name="height_request">109</property>
            <property name="visible">True</property>
            <!-- <property name="pixbuf">mayhem-logo.neon.orange-500x109.jpg</property> -->
          </object>
          <packing>
            <property name="position">0</property>
          </packing>
        </child>
        <child>
          <object class="GtkProgressBar" id="progress_splash">
            <property name="visible">True</property>
            <property name="text" translatable="yes">Configuring the Mayhem Launcher, please wait...</property>
          </object>
          <packing>
            <property name="position">1</property>
          </packing>
        </child>
        <child>
          <object class="GtkLabel" id="lbl_substatus">
            <property name="visible">True</property>
          </object>
          <packing>
            <property name="position">2</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
</interface>
